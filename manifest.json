{  

    "manifest_version": 2,
    "name": "Wikipedia Dark Mode - Visually Accessible",
    "version": "1.0",
  
    "description": "Dark mode for Wikipedia that strives to be more accessible via more visually digestible contrast. ",
    "homepage_url": "https://github.com/Josh5To/WikipediaDark",

    "icons": {
      "48": "icons/darkWicon.png"
    },

    "content_scripts": [
      {
        "matches": ["*://*.wikipedia.org/*", "*://*.wiktionary.org/*", "*://*.twitter.com/*"],
        "js": ["WikiDark.js"],
        "css": ["WikiDarkStyle.css"],
        "run_at": "document_start"
      }
    ]
  
  }